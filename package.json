{
  "name": "blackboard",
  "version": "0.1.0",
  "private": true,
  "proxy": "http://localhost:3001",
  "scripts": {
    "commit": "npx git-cz",
    "start": "npm run db:setup && npm run dev",
    "dev": "concurrently --raw -k \"npm run server:dev\" \"npm run client:start\"",
    "db:setup": "node -r dotenv/config db/setup.js",
    "server:start": "node -r dotenv/config server/server-bootstrap.js",
    "server:dev": "nodemon -r dotenv/config server/server-bootstrap.js",
    "client:start": "react-scripts start",
    "client:build": "react-scripts build",
    "test": "mocha -r dotenv/config -r @babel/register -r test/enzyme-config.js 'test/*/*.test.js'"
  },
  "dependencies": {
    "debug": "^4.1.1",
    "dotenv": "^8.0.0",
    "express": "^4.17.3",
    "jsonwebtoken": "^8.5.1",
    "lodash.throttle": "^4.1.1",
    "mongoose": "^5.5.12",
    "node-fetch": "^2.6.0",
    "p-limit": "^2.2.0",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-scripts": "3.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.4.5",
    "@babel/plugin-transform-runtime": "^7.4.4",
    "@babel/preset-env": "^7.4.5",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.4.4",
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "concurrently": "^4.1.0",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.13.2",
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.17.3",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "^7.13.0",
    "jsdom": "^15.1.1",
    "mocha": "^6.1.4",
    "nodemon": "^1.19.1",
    "sinon": "^7.3.2"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
